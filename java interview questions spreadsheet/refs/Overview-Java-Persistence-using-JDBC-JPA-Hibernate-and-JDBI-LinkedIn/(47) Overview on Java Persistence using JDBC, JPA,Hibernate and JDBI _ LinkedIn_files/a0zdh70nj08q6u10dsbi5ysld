this.trackingService.fireTrackingPayload(o.UPLOAD_TRACKING_EVENTS.UPLOAD_STATUS,{digitalMediaArtifactUrn:s,uploadSessionTrackingId:n,uploadMechanism:a,fileSizeInBytes:r,uploadState:e,uploadUseCase:"UNKNOWN",durationSinceUploadStarted:m,mediaFileInfo:i(t),mediaContentCreationSessionTrackingObject:this._createMediaContentCreationSessionTrackingObject(),videoDuration:c})}},{key:"fireUploadStatusEventStarted",value:function(){this._fireUploadStatusEvent(o.UPLOAD_TRACKING_STATES.STARTED)}},{key:"fireUploadStatusEventInProgress",value:function(){this._fireUploadStatusEvent(o.UPLOAD_TRACKING_STATES.IN_PROGRESS)}},{key:"fireUploadStatusEventFailed",value:function(){this._fireUploadStatusEvent(o.UPLOAD_TRACKING_STATES.FAILED)
this.reset()}},{key:"fireUploadStatusEventCompleted",value:function(){this._fireUploadStatusEvent(o.UPLOAD_TRACKING_STATES.COMPLETED)
this.reset()}},{key:"fireUploadStatusEventCancelled",value:function(){this._fireUploadStatusEvent(o.UPLOAD_TRACKING_STATES.CANCELLED)}},{key:"fireUploadStatusEventRefreshed",value:function(){this._fireUploadStatusEvent(o.UPLOAD_TRACKING_STATES.REFRESHED)}},{key:"firePartUploadCompletedEvent",value:function(e,t){var n=t.request,a=Date.now()-n.uploadStartTime,i=t.attempts?t.attempts-1:0,s=this.uploadSessionTrackingId,l=this.digitalMediaArtifactUrn,c=this.uploadMechanism
this.trackingService.fireTrackingPayload(o.UPLOAD_TRACKING_EVENTS.PART_COMPLETED,{digitalMediaArtifactUrn:l,uploadSessionTrackingId:s,uploadMechanism:c,partUploadMetadata:r(e,t),uploadDuration:a,numberOfRetries:i})}},{key:"firePartUploadFailureEvent",value:function(e,t){var n=t.request,a=n.responseText,i=n.status,l=o.PART_UPLOAD_TRACKING_ERRORS.UNKNOWN
i>=500?l=o.PART_UPLOAD_TRACKING_ERRORS.SERVER_ERROR:i>=400?l=o.PART_UPLOAD_TRACKING_ERRORS.CLIENT_ERROR:t.state.is.CANCELLED?l=o.PART_UPLOAD_TRACKING_ERRORS.USER_CANCELLED:0===i&&(l=o.PART_UPLOAD_TRACKING_ERRORS.NETWORK_ERROR)
var c=Date.now()-n.uploadStartTime,m=t.progress.completed||0,d=this.uploadSessionTrackingId,u=this.digitalMediaArtifactUrn,p=this.uploadMechanism
this.trackingService.fireTrackingPayload(o.UPLOAD_TRACKING_EVENTS.PART_FAILURE,{digitalMediaArtifactUrn:u,uploadSessionTrackingId:d,uploadMechanism:p,partUploadMetadata:r(e,t),responseBody:s(a),bytesTransferred:m,uploadDuration:c,errorType:l,statusCode:i})}}])
return e})()
e.default=l})

