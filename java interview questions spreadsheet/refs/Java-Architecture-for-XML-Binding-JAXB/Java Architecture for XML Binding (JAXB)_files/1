function determineShowSurvey(probability)
{return(probability>Math.random()*99);}
function onChatAvailabilityRequest(type,args,instance)
{instance.apiCall=true;instance.chatAvailability(null,instance);}
function onChatAvailabilityResponse(type,args,instance)
{var result=args[0].data;var apiCall=instance.apiCall;var callback=args[1].callback;var scope=args[1].scope;if(apiCall||callback){if(callback){callback.call(scope,result);}
